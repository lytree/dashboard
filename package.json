{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_SERVICE_ENV=dev vite",
    "dev:test": "cross-env VITE_SERVICE_ENV=test vite",
    "dev:prod": "cross-env VITE_SERVICE_ENV=prod vite",
    "build": "npm run typecheck && cross-env VITE_SERVICE_ENV=prod vite build",
    "build:dev": "npm run typecheck && cross-env VITE_SERVICE_ENV=dev vite build",
    "build:test": "npm run typecheck && cross-env VITE_SERVICE_ENV=test vite build",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "esno": "esno",
    "cleanup": "esno ./scripts/cleanup.ts",
    "compress": "esno ./scripts/compress.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.0",
    "@vueuse/core": "^9.10.0",
    "axios": "^1.2.2",
    "clipboard": "^2.0.11",
    "colord": "^2.9.3",
    "dayjs": "^1.11.7",
    "element-plus": "^2.2.28",
    "pinia": "^2.0.28",
    "qs": "^6.11.0",
    "swiper": "^8.4.5",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.3",
    "@iconify/vue": "^4.0.2",
    "@types/node": "^18.11.18",
    "@types/qs": "^6.9.7",
    "@unocss/preset-uno": "^0.47.6",
    "@unocss/vite": "^0.47.6",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "compressing": "^1.6.3",
    "cross-env": "^7.0.3",
    "esno": "^0.16.3",
    "mockjs": "^1.1.0",
    "node-html-to-image": "^3.2.4",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.9.0",
    "typescript": "^4.9.4",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-icons": "^0.14.15",
    "vite": "^4.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-progress": "^0.0.6",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.0.24",
    "zx": "^7.1.1"
